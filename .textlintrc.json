{
  "plugins": {},
  "filters": {},
  "rules": {
    "textlint-rule-no-todo": true,
    "textlint-rule-no-mixed-zenkaku-and-hankaku-alphabet": true,
    "preset-ja-technical-writing": {
      "no-mix-dearu-desumasu": {
        "preferInHeader": "",
        "preferInBody": "ですます",
        "preferInList": "ですます",
        "strict": false
      }
    },
    "preset-ja-spacing": {},
    "prh": {
      "rulePaths": ["docs/.vitepress/textlint/prh.yml"]
    }
  }
}
